{
  "devDependencies": {
    "colors": "^1.1.2",
    "concurrently": "^3.5.0",
    "coveralls": "^3.0.0",
    "istanbul-combine": "^0.3.0",
    "istanbul-merge": "^1.1.1",
    "jsdoc": "^3.5.5",    
    "jsdoc-sphinx": "0.0.6",
    "lerna": "^2.4.0",
