{
  "name": "cicero-core",
  "version": "0.0.11",
  "description": "Cicero Core - implementation of Accord Protocol Template Specification",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "prepublish": "nearleyc ./lib/tdl.ne -o ./lib/tdl.js",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json",
    "test": "nyc mocha --recursive -t 10000",
    "browserify": "browserify ./index.js -t [ babelify --presets [ latest ] ] > ./out/cicero-core.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/cicero.git"
  },
  "keywords": [
    "accord",
    "cicero",
    "smart",
    "contract"
  ],
  "author": "acccordproject.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/accordproject/cicero/issues"
  },
  "homepage": "https://github.com/accordproject/cicero#readme",
  "devDependencies": {
    "babel-preset-latest": "^6.24.1",
    "babelify": "^7.3.0",
    "browserfs": "^1.1.0",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-things": "^0.2.0",
    "eslint": "^4.6.1",
    "jsdoc": "^3.5.5",
    "license-check": "^1.1.5",
    "mocha": "^3.4.2",
    "mockery": "^2.0.0",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1"
  },
  "dependencies": {
    "composer-common": "0.14.2",
    "config": "^1.24.0",
    "debug": "^2.6.2",
    "jszip": "^3.1.4",
    "minimatch": "^3.0.4",
    "moment": "^2.18.1",
    "moo": "^0.4.3",
    "nearley": "^2.11.0",
    "winston": "^2.3.1"
  },
  "license-check-config": {
    "src": [
      "**/*.js",
      "!./test/data/**/*",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./lib/tdl.js"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 78,
    "branches": 33,
    "functions": 75,
    "lines": 78
  }
}
